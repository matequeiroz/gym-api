{
  "name": "gym-api",
  "version": "1.0.0",
  "main": "src/server.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "start:prod": "node build/server.cjs",
    "build": "tsup src --out-dir build --clean",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "25.0.3",
    "@types/pg": "8.16.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.5.0",
    "jiti": "2.6.1",
    "prettier": "3.7.4",
    "prisma": "7.2.0",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1"
  },
  "dependencies": {
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "dotenv": "17.2.3",
    "fastify": "5.6.2",
    "pg": "8.16.3",
    "zod": "4.2.1"
  }
}
